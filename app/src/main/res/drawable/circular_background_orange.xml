<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer Circle (Black Outline) -->
    <item>
        <shape android:shape="oval">
            <stroke android:color="#000000" android:width="2dp"/> <!-- Black outline -->
        </shape>
    </item>
    <!-- Inner Circle (Red Fill) -->
    <item android:right="4dp" android:left="4dp" android:top="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="#FFA500" /> <!-- Red fill -->
        </shape>
    </item>
</layer-list>
